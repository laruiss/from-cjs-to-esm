{
  "name": "@cjs-to-esm/client",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "clean:dist": "rimraf dist",
    "build": "run-p clean:dist type-check build-only",
    "build-only": "vite build",
    "dev": "vite",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "pretest:e2e": "npm run build",
    "pretest:e2e:ci": "npm run build",
    "preview": "vite preview --host --port 5050",
    "test": "run-s test:unit test:ct:ci build test:e2e:ci",
    "test:ct": "cypress open --component",
    "test:ct:ci": "cypress run --component",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@cjs-to-esm/shared": "workspace:*",
    "@gouvfr/dsfr": "^1.11.0",
    "@gouvminint/vue-dsfr": "^5.8.0",
    "focus-trap": "^7.5.4",
    "focus-trap-vue": "^4.0.3",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@cypress/vue": "^6.0.0",
    "@rushstack/eslint-patch": "^1.7.2",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.0.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.6.3",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.21.0",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.12",
    "vite-plugin-pwa": "^0.17.5",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27",
    "workbox-window": "^7.0.0"
  }
}
